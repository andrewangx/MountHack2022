{
    "manifest_version": 3,
  
    "name": "MountainHack",
    "description": "Keep track of your tasks and help complete your goals! ",
    "version": "1.0.0",
    "icons":{
      "16": "/image/16px - Extension Logo.png",
      "48": "/image/48px - Extension Logo.png",
      "128": "/image/128px - Extension Logo.png"
    },
    "permissions": ["activeTab", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon":{
          "16": "/image/16px - Extension Logo.png",
          "48": "/image/48px - Extension Logo.png",
          "128": "/image/128px - Extension Logo.png"
        }
      }
  }